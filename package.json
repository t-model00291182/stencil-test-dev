{
  "name": "stencil-dev",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-dev/stencil-dev.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/stenciljs/component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest -c jest.config.js",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "lint": "eslint src",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^4.28.2"
  },
  "devDependencies": {
    "@stencil-community/eslint-plugin": "^0.9.0",
    "@stencil/sass": "^3.2.0",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-tsdocstandard": "^17.1.2",
    "eslint-plugin-file-progress": "^1.4.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-unused-imports": "^3.2.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-jasmine2": "^29.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "ts-jest": "^29.2.5"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/stencil-dev/stencil-dev.esm.js",
      "require": "./dist/stencil-dev/stencil-dev.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  }
}
